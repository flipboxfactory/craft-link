{
  "name": "flipboxfactory/craft-link",
  "description": "Link Field Type for Craft CMS",
  "version": "2.0.0",
  "type": "craft-plugin",
  "keywords": [
    "flipbox",
    "links",
    "craftcms",
    "yii2"
  ],
  "license": "proprietary",
  "authors": [{
    "name": "Flipbox Factory",
    "email": "hello@flipboxfactory.com"
  }],
  "support": {
    "email": "support@flipboxfactory.com",
    "issues": "https://github.com/flipboxfactory/craft-link/issues?state=open",
    "source": "https://github.com/flipboxfactory/craft-link",
    "docs": "https://github.com/flipboxfactory/craft-link",
    "rss": "https://github.com/flipboxfactory/craft-link/commits/master.atom"
  },
  "require": {
    "php": "^8.0",
    "craftcms/cms": "^4.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.2",
    "phpunit/phpunit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\craft\\link\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\craft\\link\\tests\\": "tests/src/"
    }
  },
  "extra": {
    "name": "Link",
    "handle": "link",
    "developer": "Flipbox Digital",
    "developerUrl": "https://www.flipboxdigital.com",
    "class": "flipbox\\craft\\link\\Link",
    "changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/craft-link/master/CHANGELOG.md",
    "documentationUrl": "https://github.com/flipboxfactory/craft-link/blob/master/README.md",
    "hasCpSettings": false,
    "hasCpSection": false,
    "schemaVersion": "2.0.0"
  }
}
